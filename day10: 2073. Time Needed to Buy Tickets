https://leetcode.com/problems/time-needed-to-buy-tickets/
class Solution {
public:
    int timeRequiredToBuy(vector<int>& tickets, int k) {
        int ans=0;
        int n=tickets.size();
        for(int i=0;i<n;i++)
        {
            ans+=min(tickets[i],tickets[k]-(k<i));
        }
        return ans;
    }
};
